{
  "compilerOptions": {
    "outDir": "../dist",
    "module": "umd",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": false,
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true
  },
  "exclude": [
    "public"
  ]
}